{
  "name": "base-lib",
  "version": "0.1.3",
  "private": false,
  "config": {
    "target": "lib/$npm_package_version"
  },
  "files": [
    "packages",
    "lib"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name base-lib --dest $npm_package_config_target packages/index.ts"
  },
  "dependencies": {
